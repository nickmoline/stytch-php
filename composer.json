{
    "name": "nickmoline/stytch-php",
    "description": "Stytch PHP SDK",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Moline",
            "email": "nick@moline.dev"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-curl": "*",
        "guzzlehttp/psr7": "^2.7",
        "guzzlehttp/guzzle": "^7.0",
        "nesbot/carbon": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "friendsofphp/php-cs-fixer": "^3.76",
        "phpunit/phpunit": "^11.0",
        "vlucas/phpdotenv": "^5.6",
        "phpunit/php-code-coverage": "^11.0",
        "larapack/dd": "^1.1",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Stytch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stytch\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
